    "%%capture --no-display\n",
    "%%sql\n",
    "\n",
    "with employee_sales AS(\n",
    "    select \n",
    "        st.staff_id,\n",
    "        concat(st.first_name,' ', st.Last_name) as 'Sales_rep',\n",
    "        sum(oi.quantity*oi.list_price) as amount_sold_2018\n",
