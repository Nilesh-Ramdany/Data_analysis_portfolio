    "%%capture --no-display\n",
    "%%sql\n",
    "\n",
    "with order_streak AS(\n",
    "    select \n",
    "        order_date,\n",
    "        order_date - Dense_rank() over (order by order_date) as streak_id\n",
    "        from sales_orders\n",
    ")\n",
    "\n",
    "select \n",
    "    MIN(order_date) as start_date,\n",
    "    Max(order_date) as end_date,\n",
    "    Max(order_date) - Min(order_date) + 1 no_consecutive_days\n",
    "from order_streak\n",
    "Group by streak_id\n",
    "Order by start_date\n",
    "Limit 10;"
   ]
