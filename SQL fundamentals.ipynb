    "    Join sales_order_items oi on s.order_id = oi.order_id\n",
    "    Join sales_staffs st on s.staff_id = st.staff_id\n",
    "    where year(order_date) = 2018\n",
    "    group by st.staff_id\n",
    "    )\n",
    "\n",
    "\n",
    "select \n",
    "    staff_id,\n",
    "    sales_rep,\n",
    "    amount_sold_2018,\n",
    "    RANK() OVER (ORDER BY amount_sold_2018 DESC) as ranking\n",
    "from employee_sales;\n",
    "\n"
